; Zero queue depth baseline measurements
[global]
ioengine=windowsaio
direct=1
size=10G
filename=D:\testfile
runtime=30s
ramp_time=5s
group_reporting=1
stonewall
thread

[4k_random_read_qd1]
rw=randread
bs=4k
iodepth=1
numjobs=1

[4k_random_write_qd1]
rw=randwrite
bs=4k
iodepth=1
numjobs=1

[128k_sequential_read_qd1]
rw=read
bs=128k
iodepth=1
numjobs=1

[128k_sequential_write_qd1]
rw=write
bs=128k
iodepth=1
numjobs=1